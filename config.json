{
	"uglify": {
		"min": {
			"output": {
				"comments": "function set in gulpfile"
			},
			"compress": {
				"global_defs": {
					"DEBUG": false
				}
			}
		},
		"nonMin": {
			"output": {
				"comments": "function set in gulpfile"
			},
			"mangle": false,
			"compress": false
		}
	},

	"beautify": {
		"indent_size": 2,
		"max_preserve_newlines": 1
	},

	"babel": {
		"externalHelpersWhitelist": [
			"classCallCheck",
			"createClass",
			"inherits",
			"possibleConstructorReturn",
			"typeof"
		]
	},

	"rollup": {
		"globals": {
			"tweenjs/src/Tween": "this.createjs && this.createjs.Tween",
			"tweenjs/src/Timeline": "this.createjs && this.createjs.Timeline"
		},
		"forceBinding": [
			"Event",
			"EventDispatcher",
			"Ticker",
			"Tween",
			"Timeline"
		],
		"exportsExtend": "this.createjs || {}"
	}
}
