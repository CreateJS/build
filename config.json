{
  "easel_path": "../EaselJS",
  "tween_path": "../TweenJS",
  "sound_path": "../SoundJS",
  "preload_path": "../PreloadJS",

  "uglifyMin": {
    "preserveComments": "function set in gulpfile",
    "compress": {
      "global_defs": {
        "DEBUG": false
      }
    }
  },

  "uglifyNonMin": {
    "mangle": false,
    "preserveComments": "function set in gulpfile",
    "compress": false
  },

  "beautify": {
    "indent_size": 2,
    "max_preserve_newlines": 1
  },

  "babel": {
    "externalHelpersWhitelist": [
      "classCallCheck", "createClass", "inherits", "possibleConstructorReturn"
    ]
  },

  "forceBinding": [ "Event", "EventDispatcher", "Ticker", "Tween", "Timeline" ],

  "rollupGlobals": {
    "tweenjs/src/Tween": "this.createjs.Tween",
    "tweenjs/src/Timeline": "this.createjs.Timeline"
  }
}
